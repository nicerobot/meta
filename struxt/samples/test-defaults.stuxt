default
  "xlst" xlst@xmlns,
  "html" html@xmlns,
  "metal" metal@xmlns
{
  a@html "http://my.href": "link text".
  a@xlst "http://my.id": "link test".

  value-of@xlst "x/b".
  value-of@html "x/b".

  control@metal "test" {
    div "${test.index}": "metal".
  }

  control@html "test" {
    div@xslt "${test.index}": "metal".
  }

}
